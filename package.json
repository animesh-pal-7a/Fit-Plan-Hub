{
  "name": "fitplanhub",
  "version": "1.0.0",
  "description": "Fitness subscription platform with trainers and users",
  "main": "server/server.js",
  "scripts": {
    "server": "node server/server.js",
    "client": "cd client && pnpm start",
    "dev": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose up --build"
  },
  "keywords": [
    "fitness",
    "subscription",
    "trainers"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.20.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
